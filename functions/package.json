    "firebase-functions": "^4.9.0",
    "openai": "^4.96.0",
    "tars-ai": "file:.."
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.31.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.5"
  },
  "private": true
